server_name test;
    listen 10.100.102.73:8000 default_server;
    
    allow *
    location / {
        proxy_pass http://localhost:8000/;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
        proxy_set_header X-Forwarded-Proto $scheme;
        proxy_set_header X-Forwarded-Host $host;
        proxy_set_header X-Forwarded-Prefix /;
    }
    server {
    
    server_name test;
    listen 10.100.102.73:8000 default_server;
    
    allow *
    location / {
        
        proxy_pass http://localhost:8000/;
        proxy_set_header X-Forwardder X-Forwarded-Prefix /;
    }
    location /process {
        proxy_pass http://localhost:8000/process;
        proxy_set_header X-Forwardder X-Forwarded-Prefix /;
    }
}

